  "source": "pragma solidity ^0.6.0;\r\n\r\ncontract Toggle {\r\n    bool lightState = false;\r\n    address owner;\r\n    constructor() public {\r\n        owner = msg.sender;\r\n    }\r\n    // modifier onlyOwner(){\r\n    //     require (msg.sender == owner);\r\n    //     _;\r\n    // }\r\n    function switchTheLight() public { // onlyOwner\r\n        if (lightState == false){\r\n            lightState = true;\r\n        } else if (lightState == true) {\r\n            lightState = false;\r\n        }\r\n    }\r\n    function getState() public view returns(bool) {\r\n        return lightState;\r\n    }\r\n}",
